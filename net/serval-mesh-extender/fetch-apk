md5sum=`md5sum < captiveportal/images/ServalChat.apk | cut -f1 -d" "`
echo "MD5 sum = $md5sum"
if [ "x$md5sum" != "x111001c856cb04534aff96e340da346b" ]; then
   rm captiveportal/images/ServalChat.apk
   wget -O captiveportal/images/ServalChat.apk http://developer.servalproject.org/files/chat/alpha/ServalChat-alpha-0.01-pre-106-g66cc9017.apk
else
   echo MD5 sum matches -- not downloading.
fi
